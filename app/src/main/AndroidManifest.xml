                android:resource="@xml/quinto_app_widget_info" />
        </receiver>
        <receiver
            android:name=".CuartoAppWidget"
            android:exported="false">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>

            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/cuarto_app_widget_info" />
        </receiver>
        <receiver
